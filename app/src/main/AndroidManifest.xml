<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.advanced.demo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".emoji.EmojiActivity"
                  android:screenOrientation="nosensor" />

        <activity android:name=".singleTask.ActivityA"
                  android:screenOrientation="nosensor"
                  android:launchMode="singleTask"/>
        <activity android:name=".singleTask.ActivityB"
                  android:screenOrientation="nosensor" />
        <activity android:name=".singleTask.ActivityC"
                  android:screenOrientation="nosensor" />
        <activity android:name=".singleTask.ActivityD"
                  android:launchMode="singleInstance"
                  android:screenOrientation="nosensor" />
        <activity android:name=".propertyAnimator.PropertyAnimatorActivity"
                  android:screenOrientation="nosensor" />
        <activity android:name=".edittextUI.EditTextUIActivity"
                  android:screenOrientation="nosensor" />
        <activity android:name=".lockDevice.LockDeviceActivity"
                  android:screenOrientation="nosensor" />
        <receiver android:name=".lockDevice.LockReceiver"
                  android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data android:name="android.app.device_admin"
                       android:resource="@xml/lock_screen" />
            <intent-filter>
                <action
                    android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
            </intent-filter>
        </receiver>

        <activity android:name=".camera.TakePicByCameraActivity"
                  android:screenOrientation="nosensor" />

        <activity android:name=".rotationSensor.RotationSensorActivity"
                  android:screenOrientation="nosensor" >
        </activity>

        <activity android:name=".contacts.ReadContactActivity"
                  android:screenOrientation="nosensor" />

        <activity android:name=".transctionView.CircleTransactionViewActivity"
                  android:screenOrientation="nosensor" />

        <activity android:name=".cameraDetect.CameraDetectActivity"
                  android:screenOrientation="nosensor" />

        <activity android:name=".cameraPreview.CameraPreviewActivity"
                  android:screenOrientation="nosensor" />
    </application>

</manifest>